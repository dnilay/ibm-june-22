<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ListLeague</servlet-name>
  	<display-name>ListLeague</display-name>
  	<description></description>
  	<servlet-class>org.example.ListLeague</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MyServlet</servlet-name>
  	<display-name>MyServlet</display-name>
  	<description></description>
  	<servlet-class>org.example.MyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ListLeague</servlet-name>
  	<url-pattern>/list_leagues.view</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MyServlet</servlet-name>
  	<url-pattern>/my_servlet.view</url-pattern>
  </servlet-mapping>
</web-app>
